/*DBCC CHECKIDENT (inputs, RESEED, 0)*/
use specific
go
delete from inputs
go 

declare @x int;
set @x = 0
set @x = (select max(inputs_pk) from inputs) +1
use [specific]
insert into inputs
Values(
		null,	/*distance*/
		1,		/*v0*/
		1,		/*theta*/
		1,		/*phi*/
		null,		/*phif*/
		1		/*T*/
	  )



update 
	inputs
set 
	v0 = @x
from 
	inputs
where 
	inputs_pk = @x

select * from inputs


